    "babel-preset-stage-2": "^6.24.1",
    "escape-html": "^1.0.3",
    "express": "^4.15.2",
    "firebase": "^3.9.0",
    "highlight.js": "^9.11.0",
    "nodemon": "^1.11.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
