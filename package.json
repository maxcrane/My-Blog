    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "escape-html": "^1.0.3",
    "express": "^4.15.2",
    "firebase": "^3.9.0",
    "highlight.js": "^9.11.0",
    "lodash": "^4.17.4",
